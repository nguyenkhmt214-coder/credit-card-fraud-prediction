      "name": "Grafana",
      "version": "10.0.0"
    },
    {
      "type": "datasource",
      "id": "redis-datasource",
      "name": "Redis",
      "version": "1.9.0"
    },
    {
      "type": "panel",
      "id": "gauge",
      "name": "Gauge",
      "version": ""
